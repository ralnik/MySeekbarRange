<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--************ SeekbarRange *****************-->
    <declare-styleable name="SeekbarRange">
        <attr name="sbr_absoluteMinValue" format="float" />
        <attr name="sbr_absoluteMaxValue" format="float" />
        <attr name="sbr_bgSeekbarRange" format="reference" />
        <attr name="sbr_barColor" format="color" />
        <attr name="sbr_barHighlightColor" format="color" />
        <attr name="sbr_left_thumb_image" format="reference" />
        <attr name="sbr_right_thumb_image" format="reference" />
        <attr name="sbr_cornerRaduis" format="float" />
    </declare-styleable>

    <!--************ SeekbarRangeAdvance настройки для нижних edits *****************-->
    <declare-styleable name="SeekbarRangeAdvance">
        <attr name="sbra_absoluteMinValue" format="float" />
        <attr name="sbra_absoluteMaxValue" format="float" />
        <attr name="sbra_bgSeekbarRange" format="reference"/>
        <attr name="sbra_barHighlightColor" format="reference" />
        <attr name="sbra_barColor" format="color" />
        <attr name="sbra_left_thumb_image" format="reference" />
        <attr name="sbra_right_thumb_image" format="reference" />
        <attr name="sbra_show_edits" format="boolean" />

        <!--************ SeekbarRange настройки для нижних edits *****************-->
        <attr name="sbra_bgEdits" format="reference" />
        <attr name="sbra_heightEdits" format="integer"/>
        <attr name="sbra_widthEdits" format="integer" />
        <attr name="sbra_fontBoldEdits" format="boolean" />
        <attr name="sbra_textSizeOfEdits" format="integer" />
        <attr name="sbra_textColorOfEdits" format="color" />
        <attr name="sbra_edits_ems" format="integer"></attr>
        <attr name="sbra_data_type" format="enum">
            <enum name="_integer" value="0" />
            <enum name="_float" value="1" />
            <enum name="_double" value="2"/>
        </attr>
    </declare-styleable>




</resources>